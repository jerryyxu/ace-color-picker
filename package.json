{
  "private": true,
  "name": "ace-color-picker",
  "version": "0.1.0",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "canvas": "^2.7.0",
    "clsx": "^1.1.1",
    "jsdom": "^16.5.3",
    "react": "^16.12.0",
    "react-test-render": "^1.1.2",
    "tinycolor2": "^1.4.2"
  },
  "devDependencies": {
    "@types/tinycolor2": "^1.4.2",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.13",
    "enzyme": "^3.11.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  }
}
